{
  "name": "server",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "find . -regex '^.*\\(__pycache__\\|\\.py[co]\\)$' -delete",
    "clean:all": "pnpm run clean && rm -rf .venv/",
    "dev": "python3 src/main.py",
    "prepare": "python3 -m pip install -r requirements.txt",
    "start": "python3 src/main.py --production"
  }
}
